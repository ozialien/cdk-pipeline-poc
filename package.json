{
  "name": "cdk-pipiline-poc",
  "version": "0.1.0",
  "bin": {
    "cdk-pipiline-poc": "bin/cdk-pipiline-poc.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "codegen:help": "openapi-generator-cli help generate",
    "codegen:server": "openapi-generator-cli generate -i oas/product.json -g spring -o codegen/server",
    "codegen:tests": "openapi-generator-cli generate -i oas/product.json -g typescript-axios --template-dir ./templates -o test",
    "codegen:postman:tests": "openapi-generator-cli generate -i oas/product.json -g postman-collection -o postman"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.15.0",
    "@stoplight/spectral": "^6.5.0",
    "@stoplight/spectral-owasp-ruleset": "^2.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "20.12.7",
    "ajv": "^8.17.1",
    "aws-cdk": "2.142.1",
    "chai": "^5.1.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.142.1",
    "constructs": "^10.0.0"
  }
}
